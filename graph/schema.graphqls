# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type EC2Instance {
  id: ID!
  public: Boolean!
  name: String!
  publicIP: String!
  privateIP: String!
  availabilityZone: String!
  osInfo: OSInfo!
  ami: AMI!
}

type AMI {
  id: ID!
}

type OSInfo {
  id: ID!
  version: String!
  buildVersion: String!
  arch: String!
  platformDistro: String!
  platformBase: String!
}

type Query {
  ec2Instances: [EC2Instance!]!
}